{
    "name": "summonshr/review-ratings",
    "description": "A simple package to add review on any laravel model",
    "keywords": [
        "laravel",
        "review",
        "ratings"
    ],
    "autoload": {
        "psr-4": {
            "Summonshr\\ReviewRatings\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Suman Shrestha",
            "email": "summonshr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.75"
    },
    "minimum-stability": "stable",
    "version": "1.0.0",
    "extra": {
        "laravel": {
            "providers": [
                "Summonshr\\ReviewRatings\\Providers\\ReviewServiceProvider"
            ]
        }
    }
}